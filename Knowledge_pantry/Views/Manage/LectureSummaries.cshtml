@model Knowledge_pantry.Models.ManageViewModels.LectureSummariesViewModel
@{
    ViewData["Title"] = "Summaries";
    ViewData.AddActivePage(ManageNavPages.LectureSummaries);
}

<strong class="text-center">@Model.UserName summaries</strong>
<form action="../CreateNewSummary/CreateNewSummary">
    <button type="submit" class="btn btn-success col-lg-1">Create</button><br />
    @foreach (var summary in Model.UserSummaries)
    {
        <br /><a>@summary.Caption</a>
    }
</form>